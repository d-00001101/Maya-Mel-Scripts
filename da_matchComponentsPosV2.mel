/*This simple proc will match the position of corresponding componentsFirst - shift select all straying cv's on deformed surface.Second - shift select the target shape.Third - argument: 0 matches world space position, 1 matches local positionfourth - run proc.*/proc da_matchComponentPosV2(int $IO){string $mySelections[] = `ls -sl -fl`;int $arraySize = `size $mySelections`;  string $shape  = $mySelections[($arraySize - 1)];int $i;for ($i = 0; $i < ($arraySize-1); $i ++)	{		string $mySuffixes[];		tokenize ($mySelections[$i]) "." $mySuffixes;		//print ($mySuffixes);		if ($IO)		{		vector $tempVec = `xform -q -os -t  ($shape + "." + $mySuffixes[1])`;		xform  -os -t ($tempVec.x) ($tempVec.y) ($tempVec.z) $mySelections[$i];		}		else		{		vector $tempVec = `xform -q -ws -t  ($shape + "." + $mySuffixes[1])`;		xform  -ws -t ($tempVec.x) ($tempVec.y) ($tempVec.z) $mySelections[$i];		}	}}